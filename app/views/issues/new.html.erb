<div class="container">
  <h1 class="text-center">Welcome to Rosemead Carwash!</h1>
  <div class="contact-form">
    <form action="">
      <div class="form-group">
        <label for="first_name_input">First Name</label>
        <input type="text" class="form-control" id="first_name_input" placeholder="First Name">
      </div>
      <div class="form-group">
          <label for="last_name_input">Last Name</label>
          <input type="text" class="form-control" id="last_name_input" placeholder="Last Name">
      </div>
      <div class="form-group">
          <label for="email_input">Email Address</label>
          <input type="email" class="form-control" id="email_input" placeholder="Email Address">
      </div>
      <div class="form-group">
          <label for="phone_number_input">Phone Number</label>
          <input type="text" class="form-control" id="phone_number_input" placeholder="Phone Number">
      </div>
      <div class="form-group">
          <label for="street_address_1_input">Street Address 1</label>
          <input type="text" class="form-control" id="street_address_1_input" placeholder="Street Address 1">
      </div>
      <div class="form-group">
          <label for="street_address_2_input">Street Address 2</label>
          <input type="text" class="form-control" id="street_address_2_input" placeholder="Street Address 2">
      </div>
      <div class="form-group">
          <label for="city_input">City</label>
          <input type="text" class="form-control" id="city_input" placeholder="City">
      </div>
      <div class="form-group">
          <label for="zip_code_input">Zip Code</label>
          <input type="text" class="form-control" id="zip_code_input" placeholder="Zip Code">
      </div>
      <div class="form-group">
          <label for="message_title_input">Message Title</label>
          <input type="text" class="form-control" id="message_title_input" placeholder="Message Title">
      </div>
      <div class="form-group">
          <label for="zip_code_input">Message</label>
          <textarea class="form-control" rows="10" class="form-control" id="message_input" placeholder="Message">
            
          </textarea>
      </div>
      <div>
        <label for="date_input">Date</label>
        <input type="date" class="form-control" id="date_input" placeholder="Date">
      </div>
    </form>
  </div>
</div>